@model IEnumerable<DigitalSphere.Models.IssuedBook>
@{
    ViewBag.Title = "IssuedBook";
    Layout = "~/Views/Shared/Librarian_Layout.cshtml";
}

<h2>IssuedBook</h2>
<h2 style="margin-top:100px">Issued Books</h2>
<div class="container">
<div class="table-responsive">
<table border="1" class="table table-striped" style="font-size:medium">
    <thead>
        <tr>
            <td>Book_id</td>
            <td>Transaction_Id</td>
            <td>Job_Id</td>
            <td>Issuedate</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var i in Model)
        {
        <tr>
            <td>@i.Book_Id</td>
            <td>@i.Transaction_Id</td>
            <td>@i.Job_Id</td>
            <td>@i.IssueDate</td>
        </tr>
        }
    </tbody>
</table>

    </div></div>